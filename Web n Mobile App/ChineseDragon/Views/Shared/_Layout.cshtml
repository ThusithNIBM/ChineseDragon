<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    
    @Styles.Render("https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback")
    @Styles.Render("~/plugins/fontawesome-free/css/all.min.css")
    @Styles.Render("~/dist/css/adminlte.min.css")
    @Styles.Render("~/plugins/sweetalert2/sweetalert2.min.css")
    @Styles.Render("~/plugins/toastr/toastr.min.css")
    
   
    <!-- Ionicons -->
   
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="col-12 col-md-6">
                
                <nav class="navbar navbar-expand navbar-primary navbar-dark">
                    <!-- Left navbar links -->
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                        </li>
                        
                        <li class="nav-item d-none d-sm-inline-block">
                            <a href="/Home/index" class="nav-link">Kitchen Workflow</a>
                        </li>
                        <li class="nav-item d-none d-sm-inline-block">
                          <a href="/Home/CashierWorkflow" class="nav-link">CashierWorkflow</a>
</li>
                    </ul>

                </nav>
            </div>
        </div>
    </div>
    
    <div class="container body-content">
        @RenderBody()
       
       
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

   
    @Scripts.Render("~/plugins/jquery/jquery.min.js")
    @Scripts.Render("~/plugins/bootstrap/js/bootstrap.bundle.min.js")
    @Scripts.Render("~/dist/js/adminlte.min.js")
        @Scripts.Render("~/dist/js/adminlte.min.js")
        @RenderSection("scripts", required: false)
   
    

</body>
</html>
