<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback")
    @Styles.Render("~/plugins/fontawesome-free/css/all.min.css")
    @Styles.Render("~/dist/css/adminlte.min.css")
    @Styles.Render("~/plugins/sweetalert2/sweetalert2.min.css")
    @Styles.Render("~/plugins/toastr/toastr.min.css")

</head>
<body>
    

    <div class="container body-content">
        @RenderBody()


        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    @Scripts.Render("~/plugins/jquery/jquery.min.js")
    @Scripts.Render("~/plugins/bootstrap/js/bootstrap.bundle.min.js")
    @Scripts.Render("~/dist/js/adminlte.min.js")
    @Scripts.Render("~/dist/js/adminlte.min.js")
    @RenderSection("scripts", required: false)



</body>
</body>
</html>
